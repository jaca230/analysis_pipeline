{
  "logger": {
    "name": "Pipeline_Logger",
    "level": "info",
    "pattern": "[%Y-%m-%d %H:%M:%S.%e] [%^%l%$] %v",
    "sinks": {
      "console": {
        "enabled": true,
        "color": true
      },
      "file": {
        "enabled": false,
        "filename": "logs/app.log",
        "max_size": 10485760,
        "max_files": 3
      }
    }
  }
}
